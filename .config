# name: .config
# date: 12 Dec 14
# auth: Zach Hartwig
# mail: hartwig@psfc.mit.edu
# desc: Configuration file for the IPyLogbook system

[IPyLogbook-Config]

# The full path to the top-level ("HEAD") directory of this
# IPyLogbook. This should be the directory from which the IPython
# notebook server is launched
head: /home/hartwig/ipython/IPyLogbook

# The full URL and ssh-port that will be used to connect to the
# IPython notebook server. If the server is run locally, then
# typically, the url:ssh-port will be 'http://localhost:8888'. If the
# server is running remotely then the URL will be the dynamically
# allocated hostname or the IP address; the SSH port will be whatever
# you have configured to listen for traffic via HTTP/HTTPS
url: http://localhost
ssh-port: 8888

# This is the name of the IPyLogbook
name: Alpha Experiment

# This is the information of the lead administrator for the Logbook
[IPyLogbook-Admin]
admin: Zaphod Beeblebrox
email: president@galaxy.com
